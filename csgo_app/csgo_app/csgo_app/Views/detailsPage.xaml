<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="csgo_app.Views.detailsPage">
    <AbsoluteLayout>
        <Label      
      x:Name ="nameL"
      FontSize="20"
      HorizontalTextAlignment="Center"
      AbsoluteLayout.LayoutBounds="0,140,1,40"
      AbsoluteLayout.LayoutFlags="XProportional,WidthProportional">
        </Label>
        <Label 
      x:Name="mapL"
      FontSize="15"
      HorizontalTextAlignment="Center"
      AbsoluteLayout.LayoutBounds="0,180,1,40"
      AbsoluteLayout.LayoutFlags="XProportional,WidthProportional">
        </Label>
        <Label      
      x:Name ="ucastL"
      FontSize="15"
      HorizontalTextAlignment="Center"
      AbsoluteLayout.LayoutBounds="0,260,1,40"
      AbsoluteLayout.LayoutFlags="XProportional,WidthProportional">
        </Label>
        <Button x:Name="DeleteObject" Text="Delete this training"/>
    </AbsoluteLayout>
</ContentPage>